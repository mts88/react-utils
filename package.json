{
  "name": "@mts88/react-utils",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mts88/react-utils.git"
  },
  "author": "MTS <fsca.motisi@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "doc:generate": "typedoc --tsconfig ./tsconfig.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "firebase": "^9.15.0"
  },
  "peerDependencies": {
    "firebase": "^9.15.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-hook-form": "^7.43.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "react-router-dom": "^6.8.1",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-utils.umd.js",
  "module": "./dist/react-utils.es.js",
  "types": "./dist/react-utils.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-utils.es.js",
      "require": "./dist/react-utils.umd.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "[![Commitizen friendly](https://img.shields.io/badge/commitizen-friendly-brightgreen.svg)](http://commitizen.github.io/cz-cli/)",
  "bugs": {
    "url": "https://github.com/mts88/react-utils/issues"
  },
  "homepage": "https://github.com/mts88/react-utils#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "keywords": [
    "react",
    "typescript",
    "utils",
    "react-typescript"
  ]
}
